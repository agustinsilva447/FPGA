#include <stdio.h>
#include "comblock.h"
#include "platform.h"
#include "xil_printf.h"
#include "xparameters.h"

int main()
{
    init_platform();

    xil_printf("10 Queens Problem -> \n");

    cbWrite(XPAR_COMBLOCK_0_AXIL_BASEADDR, CB_OREG0, 1);
    xil_printf("Done: %U \n", cbRead(XPAR_COMBLOCK_0_AXIL_BASEADDR, CB_IREG0));
    xil_printf("Soluciónes: %U \n", cbRead(XPAR_COMBLOCK_0_AXIL_BASEADDR, CB_IREG1));
    xil_printf("Reset: %U \n", cbRead(XPAR_COMBLOCK_0_AXIL_BASEADDR, CB_OREG0));

    cbWrite(XPAR_COMBLOCK_0_AXIL_BASEADDR, CB_OREG0, 0);
    xil_printf("Done: %U \n", cbRead(XPAR_COMBLOCK_0_AXIL_BASEADDR, CB_IREG0));
    xil_printf("Soluciónes: %U \n", cbRead(XPAR_COMBLOCK_0_AXIL_BASEADDR, CB_IREG1));
    xil_printf("Reset: %U \n", cbRead(XPAR_COMBLOCK_0_AXIL_BASEADDR, CB_IREG2));

    while(!(cbRead(XPAR_COMBLOCK_0_AXIL_BASEADDR, CB_IREG0)&0x01));
    xil_printf("Done: %U \n", cbRead(XPAR_COMBLOCK_0_AXIL_BASEADDR, CB_IREG0));
    xil_printf("Soluciónes: %U \n", cbRead(XPAR_COMBLOCK_0_AXIL_BASEADDR, CB_IREG1));
    xil_printf("Reset: %U \n", cbRead(XPAR_COMBLOCK_0_AXIL_BASEADDR, CB_IREG2));

    cleanup_platform();
    return 0;
}
